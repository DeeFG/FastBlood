<div class="container">

    <ul>
    {{#each patients}}
    <li>{{this.FirstName}}</li>
    {{/each}}
    </ul>

    <!-- header -->
    <div class="row">
        <div class="col-md-12">
            <nav class="navbar navbar-expand-lg navbar-light bg-light">
                <a class="navbar-brand" href="#">Navbar</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
                    aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-item active">
                            <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Features</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Pricing</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Disabled</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
    </div>
    <br><br>
    <div class="row .search">
        <div class="col-md-12">
            <h2>Progress Bar</h2>
            <br><br>
            <div class="row">
                <div class="col-md-12" id="staticData">
                </div>
            </div>
            <div class="row">
                <div class="inputBox col-md-12">
                    <!-- Find patient -->
                    <div class="form-row">
                        <div class="form-group col-md-3 patientInfo">
                            <label for="">Enter the patient ID</label>
                            <input id="id-input" type="text" class="form-control" placeholder="Hospital ID number">
                        </div>
                        <div class="form-group col-md-12">
                            <button id="submitPatient" class="btn btn-primary">Search Patient</button>
                        </div>
                    </div>

                    <div class="progress" style="height: 100px;">
                        <div class="progress-bar" role="progressbar" style="width: 0%" aria-valuenow="15"
                            aria-valuemin="0" aria-valuemax="100">TYPE and Screen</div>
                        <div class="progress-bar2 bg-success" role="progressbar" style="width: 0%" aria-valuenow="30"
                            aria-valuemin="0" aria-valuemax="100">Type Confirm</div>
                        <div class="progress-bar3 bg-info" role="progressbar" style="width: 0%" aria-valuenow="20"
                            aria-valuemin="0" aria-valuemax="100">Finding and Testing your units</div>
                    </div>

                    </div>
                </div>

            </div>

        </div>
    </div>
    <!--  -->
</div>
</div>
<br><br>
<!--  -->

<footer> footer </footer>
<!--  -->
</div>

<button class="goTesting">NEXT</button>
<button class="goProducts">NEXT</button>
<button class="goAntibodies">NEXT</button>